extends layout

block content

	.row
		.small-6.large-6.columns
			h1= title
		.small-4.large-4.columns.center
			p
		.small-2.large-2.columns
			a.success.button(href="/", id="relaod") Home
			a.success.button(href="/score" ) Score final
	.row
		.small-12.columns
			table
				thead
					tr
						th Nom
						th Score
						th Victoir
				tbody
				each data in datas
					tr
						td= data.nom
						td= data.score
						td= data.victoir
