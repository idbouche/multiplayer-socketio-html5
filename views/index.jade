extends layout

block content


	.row
		.small-6.large-6.columns
			h1= title
		.small-4.large-4.columns.center
			p
		.small-2.large-2.columns
			a.button(href="/score" ) Score
			a.relaod.alert.button(href="/", id="relaod" ) Stop
	.row
		.small-3.columns
			form(id='form')
				label User
				input(type="text" id="nom")
				input.success.button(type="submit" id="go" value="Entrez")
			p= message
		.small-3.columns
			#list
	.row
		.small-2.large-2.columns
			p#nom1 Nom :
		.small-8.large-8.columns.center
			p VS
		.small-2.large-2.columns
			p#nom2 Nom :
	.small-12.large-12.columns.center
		p#tImer
		p#gameOver
		canvas#cv( width="800" height="500" style="border:1px solid #000000;")
